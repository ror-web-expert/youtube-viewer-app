<div class="mx-auto md:w-2/3 w-full flex">
  <div class="mx-auto">
    <div id="<%= dom_id @job %>">
      <p class="my-5">
        <strong class="block font-medium mb-1">Board:</strong>
        <%= @job.board&.title %>
      </p>

      <p class="my-5">
        <strong class="block font-medium mb-1">Title:</strong>
        <%= @job.title %>
      </p>

      <p class="my-5">
        <strong class="block font-medium mb-1">Scraped url:</strong>
        <%= @job.scraped_url %>
      </p>

      <p class="my-5">
        <strong class="block font-medium mb-1">Status:</strong>
        <%= @job.status %>
      </p>

      <p class="my-5">
        <strong class="block font-medium mb-1">Response data:</strong>
        <%#= post.response_data %>
        <% @job.response_data.each do |key, value| %>
        <div class="mb-4">
          <h2 class="text-lg font-semibold mb-2 font-bold"><%= key.titleize %> : </h2>
          <p><%= value&.html_safe %></p>
        </div>
      <% end %>
      </p>
    </div>
  </div>
</div>
